<h1>New Domain</h1>
<p id="notice"><%= notice %></p>


<%= form_for :domain, url: domains_path do |f| %>
  <p>
    <%= f.label :name%><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <button name='button' type='submit'>Submit</button>
  </p>
</div>
<h3>Menu</h3>
<%= link_to 'See All Domains', controller: 'domains' %>
  
<% end %>