<p id="notice"><%= notice %></p>


<%= link_to 'New Domain', new_domain_path %> <br>

<%= @domains.count %> domains

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Uacode</th>
      <th>Pub ID</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @domains.each do |domain| %>
    
      <tr>
        <td><%= link_to domain.name, domain_path(domain) %></td>
        <td><%= domain.uacode %></td>
        <td><%= domain.pubid %></td>
        <td><%= domain.author.username %></td>
        <td><%= link_to 'Edit', edit_domain_path(domain) %></td>
        <td><%= link_to 'Destroy', domain, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<br>


