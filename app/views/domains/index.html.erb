<p id="notice"><%= notice %></p>


<%= link_to 'New Domain', new_domain_path %> <br>
<%= link_to 'Google', 'https://www.google.com/', :target => "_blank" %>
<% if logged_in? %>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Uacode</th>
      <th>Pub ID</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @domains.each do |domain| %>
    
      <tr>
        <td><%= link_to domain.name, domain_path(domain) %></td>
        <td><%= domain.uacode %></td>
        <td><%= domain.pubid %></td>
        <td><%= link_to 'Edit', edit_domain_path(domain) %></td>
        <td><%= link_to 'Destroy', domain, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<% else %>
<br>
<h2>Please login for this data</h2>
<br>
<% end %>

<br>


